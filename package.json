{
  "name": "dclib",
  "description": "Eth lib",
  "version": "3.0.0-beta.0",
  "author": "name <to@alexstep.com>",
  "private": false,
  "homepage": "./",
  "scripts": {
    "preinstall": "cd ./packages/dc-messaging/ && yarn",
    "start": "node scripts/start.js",
    "eslint": "eslint --ext=.js,.tag ./src",
    "lint-staged": "lint-staged",
    "prebuild": "npm run eslint",
    "build": "node scripts/build.js",
    "postbuild": "node scripts/postBuild.js",
    "release": "release-it premajor --preReleaseId=beta ",
    "docs": "esdoc && serve ./docs",
    "docs_generate": "esdoc",
    "docs_show": "serve ./docs"
  },
  "pre-commit": [
    "lint-staged"
  ],
  "keywords": [
    "DCLib"
  ],
  "main": "./src/index.js",
  "lint-staged": {
    "src/*.js": [
      "eslint"
    ],
    "*.tag": [
      "eslint"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,tag}"
    ],
    "setupFiles": [
      "<rootDir>/scripts/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/src/**/__tests__/**/*.js?(x)",
      "<rootDir>/src/**/?(*.)(spec|test).js?(x)"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tag$": "riot-jest-transformer",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/scripts/config/jest/cssTransform.js",
      "^(?!.*\\.(js|tag|css|json)$)": "<rootDir>/scripts/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "dependencies": {
    "big-integer": "^1.6.23",
    "event-emitter": "^0.3.5",
    "web3": "^1.0.0-beta.30"
  },
  "devDependencies": {
    "archiver": "^2.1.0",
    "autoprefixer": "7.1.0",
    "babel-core": "6.24.1",
    "babel-eslint": "7.2.3",
    "babel-jest": "20.0.3",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2015-riot": "^1.1.0",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-runtime": "6.23.0",
    "case-sensitive-paths-webpack-plugin": "1.1.4",
    "chalk": "1.1.3",
    "css-loader": "^0.28.4",
    "dotenv": "4.0.0",
    "esdoc": "^1.0.3",
    "esdoc-exclude-source-plugin": "^1.0.0",
    "esdoc-inject-script-plugin": "^1.0.0",
    "esdoc-standard-plugin": "^1.0.0",
    "eslint": "^4.16.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.33.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "5.0.3",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-riot": "^0.1.6",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "2.1.0",
    "file-loader": "0.11.1",
    "fs-extra": "3.0.1",
    "html-webpack-plugin": "2.28.0",
    "inquirer": "^4.0.1",
    "install": "^0.10.1",
    "jest": "20.0.3",
    "lerna": "^2.9.0",
    "less": "^2.7.2",
    "less-loader": "^4.0.4",
    "lint-staged": "^4.3.0",
    "npm": "^5.5.1",
    "object-assign": "4.1.1",
    "postcss-flexbugs-fixes": "3.0.0",
    "postcss-loader": "2.0.5",
    "pre-commit": "^1.2.2",
    "promise": "7.1.1",
    "react-dev-utils": "^3.0.0",
    "release-it": "^5.0.0",
    "riot-hot-reload": "^1.0.0",
    "riot-jest-transformer": "^1.0.10",
    "riot-tag-loader": "^1.0.0",
    "serve": "^5.2.1",
    "style-loader": "^0.18.1",
    "svg-inline-loader": "^0.7.1",
    "sw-precache-webpack-plugin": "0.9.1",
    "url-loader": "0.5.8",
    "webpack": "2.6.1",
    "webpack-bundle-size-analyzer": "^2.7.0",
    "webpack-dev-server": "2.4.5",
    "webpack-manifest-plugin": "1.1.0",
    "whatwg-fetch": "2.0.3"
  }
}
